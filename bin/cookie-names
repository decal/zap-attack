#!/usr/bin/env ruby
# encoding: utf-8
#
# Form a unique list of names from HTTP response header fields 
#

require 'zap_attack'
require 'ipaddr'

include ZapAttack::API

params = Params.new
cookies = params.select { |h| h['type'] == 'cookie' }
cnames = []

cookies.each do |c|
  cnames << c['name']
end

cnames.sort!
cnames.uniq!

puts cnames

exit 0
